{
  "name": "express-starter-boilerplate-w-passport",
  "version": "0.0.0",
  "private": true,
  "author": "Bojan Jagetic",
  "scripts": {
    "start": "NODE_ENV=development node init.js",
    "build": "NODE_ENV=production node init.js",
    "lint": "node_modules/.bin/eslint ./src/**/**.js",
    "lint:fix": "node_modules/.bin/eslint ./src/**/**.js --fix",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "test:cover": "yarn run test --coverage",
    "cypress:open": "cypress open",
    "precommit": "yarn run lint && yarn test",
    "prepush": "yarn run lint",
    "heroku-prebuild": "echo This runs before Heroku installs your dependencies.",
    "heroku-postbuild": "echo This runs afterwards."
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "connect-redis": "^4.0.3",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "gulp": "^4.0.2",
    "gulp-apidoc": "^0.2.8",
    "gulp-apidoc-swagger": "^0.2.2",
    "gulp-mocha": "^7.0.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.6.6",
    "morgan": "~1.9.1",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pm2": "^4.2.0",
    "q": "^1.5.1",
    "redis": "^2.8.0",
    "run-sequence": "^2.2.1",
    "swagger": "^0.7.5",
    "validator": "^12.0.0"
  },
  "devDependencies": {
    "connect-mongo": "^3.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "express-jwt": "^5.3.1"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "es6": true,
      "browser": true
    },
    "rules": {
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "comma-dangle": [
        "error",
        "never"
      ],
      "no-unused-vars": [
        "warn"
      ],
      "no-var": [
        "off"
      ],
      "one-var": [
        "off"
      ]
    }
  }
}
